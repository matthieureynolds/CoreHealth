    "@react-navigation/stack": "^7.3.3",
    "@supabase/supabase-js": "^2.50.0",
    "expo": "~53.0.10",
    "expo-auth-session": "^6.2.0",
    "expo-crypto": "^14.1.5",
    "expo-document-picker": "~13.1.5",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-location": "^18.1.5",
    "expo-sensors": "^14.1.4",
    "expo-status-bar": "~2.2.3",
    "openai": "^5.5.1",
