{
  "name": "corehealth",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:dev": "NODE_ENV=development expo start",
    "start:staging": "NODE_ENV=staging expo start",
    "start:prod": "NODE_ENV=production expo start",
    "android": "expo start --android",
    "android:dev": "NODE_ENV=development expo start --android",
    "android:staging": "NODE_ENV=staging expo start --android",
    "android:prod": "NODE_ENV=production expo start --android",
    "ios": "expo start --ios",
    "ios:dev": "NODE_ENV=development expo start --ios",
    "ios:staging": "NODE_ENV=staging expo start --ios",
    "ios:prod": "NODE_ENV=production expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "code-quality": "npm run lint && npm run format:check && npm run type-check",
